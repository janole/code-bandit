{
	"name": "@janole/code-bandit",
	"version": "0.1.7",
	"author": "Jan Ole Suhr <ole@janole.com> (https://janole.com)",
	"homepage": "https://github.com/janole/code-bandit?#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/janole/code-bandit.git"
	},
	"license": "MIT",
	"bin": {
		"coba": "dist/coba.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"dev": "node --import @swc-node/register/esm-register ./src/coba.tsx",
		"watch": "tsc --watch",
		"typecheck": "npx tsc --noEmit -p tsconfig.json",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"prebuild": "npm run genversion && patch-package",
		"build": "rimraf dist && node esbuild.config.js",
		"genversion": "echo \"export const VERSION = \\\"${npm_package_version}\\\";\" > src/.version.ts",
		"clean": "rimraf dist",
		"prepack": "npm run build"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@langchain/anthropic": "^0.3.24",
		"@langchain/core": "^0.3.62",
		"@langchain/google-genai": "^0.2.14",
		"@langchain/ollama": "^0.2.3",
		"@langchain/openai": "^0.5.18",
		"commander": "^14.0.0",
		"fast-glob": "^3.3.3",
		"ink": "^6.0.1",
		"langchain": "^0.3.29",
		"marked": "^15.0.12",
		"marked-terminal": "^7.3.0",
		"react": "^19.1.0",
		"ulid": "^3.0.1",
		"write-file-atomic": "^6.0.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^7.0.0",
		"@stylistic/eslint-plugin": "^5.1.0",
		"@swc-node/register": "^1.10.10",
		"@types/marked": "^5.0.2",
		"@types/marked-terminal": "^6.1.1",
		"@types/node": "^24.0.10",
		"@types/react": "^19.1.8",
		"@types/write-file-atomic": "^4.0.3",
		"@typescript-eslint/eslint-plugin": "^8.36.0",
		"esbuild": "^0.25.6",
		"eslint": "^9.30.1",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-unicorn": "^59.0.1",
		"globals": "^16.3.0",
		"patch-package": "^8.0.0",
		"rimraf": "^6.0.1",
		"typescript": "^5.8.3"
	}
}
